{
  "name": "@omicronenergy/oscd-editor-ied",
  "version": "0.0.0",
  "description": "OpenSCD Editor Plugin for Editing the IEDs in an IEC 61850 document.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OMICRONEnergyOSS/oscd-editor-ied.git"
  },
  "bugs": {
    "url": "https://github.com/OMICRONEnergyOSS/oscd-editor-ied/issues"
  },
  "homepage": "https://openscd.org",
  "keywords": [
    "OpenSCD",
    "iec81650"
  ],
  "license": "Apache-2.0",
  "exports": {
    ".": "./dist/oscd-editor-ied.js",
    "./oscd-editor-ied.js": "./dist/oscd-editor-ied.js"
  },
  "files": [
    "/dist/",
    "!/dist/*.spec.*",
    "!/dist/*.test.*"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "analyze": "cem analyze --litelement --exclude demo/* --exclude dist/* --exclude *.spec.ts --exclude *.test.ts --exclude coverage/*",
    "start": "npm run build && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds --node-resolve --hmr \"",
    "start:bundle": "npm run bundle && concurrently -k -r \"rollup -c rollup.config.js --watch\" \"wds --watch \"",
    "build": "rimraf dist && mkdir dist && npm run analyze -- --exclude dist && tsc ",
    "bundle": "rimraf dist && rollup -c rollup.config.js",
    "test": "npm run build && wtr --coverage --group unit",
    "test:watch": "npm run build && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch --group unit\"",
    "test:visual": "npm run build && playwright install && wtr --group visual",
    "test:update": "npm run build && wtr --group visual --update-visual-baseline",
    "deploy": "npm run bundle && gh-pages -d dist -b deploy",
    "preinstall": "git submodule update --init --recursive --remote",
    "prepare": "husky"
  },
  "dependencies": {
    "@lit/localize": "^0.12.2",
    "@omicronenergy/oscd-edit-dialog": "file:../oscd-edit-dialog",
    "@omicronenergy/oscd-menu-open": "^0.0.6",
    "@omicronenergy/oscd-menu-save": "^0.0.2",
    "@omicronenergy/oscd-shell": "^0.0.10",
    "@omicronenergy/oscd-ui": "file:../oscd-ui",
    "@open-wc/scoped-elements": "^3.0.5",
    "@openscd/oscd-api": "^0.1.6",
    "@openscd/scl-lib": "^2.0.0",
    "@webcomponents/scoped-custom-element-registry": "^0.0.10",
    "ace-custom-element": "^1.6.5",
    "lit": "^3.3.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@custom-elements-manifest/analyzer": "^0.11.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@lit/localize-tools": "^0.8.0",
    "@open-wc/dev-server-hmr": "^0.2.0",
    "@open-wc/eslint-config": "^13.0.0",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-polyfill": "^1.0.6",
    "@web/rollup-plugin-html": "^2.4.1",
    "@web/rollup-plugin-import-meta-assets": "^2.3.0",
    "@web/test-runner": "0.20.2",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-playwright": "0.11.1",
    "@web/test-runner-visual-regression": "0.10.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-lit-a11y": "^5.1.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tsdoc": "^0.5.0",
    "eslint-plugin-wc": "^3.0.2",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.3",
    "rollup-plugin-copy": "^3.5.0",
    "sinon": "^21.0.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "@web/test-runner-playwright": {
      "playwright": "1.54.1"
    }
  },
  "customElements": "custom-elements.json",
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "eslint"
    ]
  }
}
